{
    "name": "Accent Press",
    "description": "Accent Press is a simple Chrome Extension that allows you to easily write accents.",
    "version": "1.0.1",
    "icons": {
        "16": "icon16-transparent.png",
        "48": "icon48-transparent.png",
        "128": "icon128-transparent.png"
    },
    "manifest_version": 3,
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["inject.js"]
        }
    ],
    "host_permissions": ["<all_urls>"]
}
